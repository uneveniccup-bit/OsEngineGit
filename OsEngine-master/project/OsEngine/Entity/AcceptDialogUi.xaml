<Window x:Class="OsEngine.Entity.AcceptDialogUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       Title="Confirmation window" Height="206" Width="482" Topmost="True" Style="{StaticResource WindowStyleNoResize}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="/Images/OsLogo.ico">
    <Grid>
        <Rectangle Margin="5,5,5,40"/>
        <Button Name="ButtonAccept" Content="Accept" FontWeight="Bold" HorizontalAlignment="Right" Margin="0,0,5,10" VerticalAlignment="Bottom" Width="235" Click="ButtonAccept_Click" />
        <Button Name="ButtonCancel" Content="Don't do it"  FontWeight="Bold" Margin="5,0,245,10" VerticalAlignment="Bottom" Click="ButtonCancel_Click" />
        <TextBox Name="TextBoxMessage" Margin="5,5,6,40" TextWrapping="Wrap" Text="TextBox" Height="NaN" FontStretch="Expanded" IsReadOnly="True" TextAlignment="Center" FontSize="15" FontWeight="Bold"/>
    </Grid>
</Window>
